         <copy source="Startup_Code/clock-cfv1.c"                      target="Startup_Code/clock.c" />
         <copy source="Project_Headers/clock_configure-cfv1.h"         target="Project_Headers/clock_configure.h" />
      </condition>
   </projectActionList>
</root>