<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKMxx -->
   <projectActionList id="Options-file-list-MKMxxZ5">
      <applyWhen>
         <or>
            <deviceSubfamilyIs>MKM14Z5</deviceSubfamilyIs>
            <deviceSubfamilyIs>MKM33Z5</deviceSubfamilyIs>
            <deviceSubfamilyIs>MKM34Z5</deviceSubfamilyIs>
         </or>
      </applyWhen>
      <variable id="Clock-setup-MKMxx" name="Clock setup code" description="Adds code for basic clock initialization - check assumptions made" />
      <variable id="UART-setup-MKMxx" name="UART setup code" description="Adds C stdio (UART) support for newlib" />
      <condition variable="Clock-setup-MKMxx">
         <copy source="Startup_Code/clock-MKMxx.c" target="Startup_Code/clock.c" />
         <copy source="Project_Headers/clock_configure-MKMxx.h" target="Project_Headers/clock_configure.h" />
      </condition>
      <condition variable="UART-setup-MKMxx">
         <copy source="Startup_Code/uart-mkm.c" target="Startup_Code/uart.c" />
         <copy source="Project_Headers/uart.h" target="Project_Headers/uart.h" />
      </condition>
   </projectActionList>
</root>