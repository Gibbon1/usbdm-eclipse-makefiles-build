<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0">
   <!-- Extra Optional files for MKL0x -->
   <projectActionList id="Options-file-list-MKL0xZ4">
      <applyWhen>
         <or>
            <deviceSubfamilyIs>MKL02Z4</deviceSubfamilyIs>
            <deviceSubfamilyIs>MKL04Z4</deviceSubfamilyIs>
            <deviceSubfamilyIs>MKL05Z4</deviceSubfamilyIs>
         </or>
      </applyWhen>
      <variable id="Clock-setup-MKL0x" name="Clock setup code" description="Adds code for basic clock initialization - check assumptions made" />
      <variable id="UART-setup-MKL0x" name="UART setup code" description="Adds C stdio (UART) support for newlib" />
      <condition variable="Clock-setup-MKL0x">
         <copy source="Startup_Code/clock-MKL0x.c" target="Startup_Code/clock.c" />
         <copy source="Project_Headers/clock_configure-MKL0x.h" target="Project_Headers/clock_configure.h" />
      </condition>
      <condition variable="UART-setup-MKL0x">
         <copy source="Startup_Code/uart-mkl.c" target="Startup_Code/uart.c" />
         <copy source="Project_Headers/uart.h" target="Project_Headers/uart.h" />
      </condition>
   </projectActionList>
</root>