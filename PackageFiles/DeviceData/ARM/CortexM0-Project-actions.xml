      <!-- Cortex-M0 CMSIS RTOS -->
      <projectActionList id="Cortex-M0-CMSIS" root="usbdm_resource_path/Stationery/ARM/" >
         <variable id="CMSIS-M0-rtos"  name="CMSIS RTOS framework"   description="Adds CMSIS-RTOS framework" />
