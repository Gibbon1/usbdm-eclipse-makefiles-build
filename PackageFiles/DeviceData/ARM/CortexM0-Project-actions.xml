      <!-- GNU Build options Cortex M0 (e.g. MKL, MKE) devices -->
      <projectActionList id="gnuSettings.cortex.m0" root="usbdm_resource_path/Stationery/ARM/" >
         <createFolder  target="Sources"                        type="" />
